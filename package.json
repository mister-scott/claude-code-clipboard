{
    "name": "claudeCodeClipboard",
    "repository": "https://github.com/mister-scott/claude-code-clipboard",
    "displayName": "Claude Code Clipboard",
    "description": "Apply code updates from AI generated content via clipboard",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:claudeCodeClipboard.applyUpdates"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "claudeCodeClipboard.applyUpdates",
                "title": "Apply Code Updates from Clipboard"
            }
        ],
        "keybindings": [
            {
                "command": "claudeCodeClipboard.applyUpdates",
                "key": "ctrl+alt+v",
                "mac": "cmd+alt+v"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "eslint src --ext ts",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/node": "^14.14.37",
        "@types/vscode": "^1.60.0",
        "@types/xmldom": "^0.1.34",
        "@typescript-eslint/eslint-plugin": "^4.22.0",
        "@typescript-eslint/parser": "^4.22.0",
        "eslint": "^7.24.0",
        "typescript": "^4.2.4"
    },
    "dependencies": {
        "@xmldom/xmldom": "^0.8.10"
    }
}
