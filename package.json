{
    "name": "code-update-extension",
    "displayName": "Code Update Extension",
    "description": "Apply code updates from clipboard",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:codeUpdate.applyUpdates"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "codeUpdate.applyUpdates",
                "title": "Apply Code Updates from Clipboard"
            }
        ],
        "keybindings": [
            {
                "command": "codeUpdate.applyUpdates",
                "key": "ctrl+alt+p",
                "mac": "cmd+alt+p"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "eslint src --ext ts",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/node": "^14.14.37",
        "@types/vscode": "^1.60.0",
        "@types/xmldom": "^0.1.34",
        "@typescript-eslint/eslint-plugin": "^4.22.0",
        "@typescript-eslint/parser": "^4.22.0",
        "eslint": "^7.24.0",
        "typescript": "^4.2.4"
    },
    "dependencies": {
        "@xmldom/xmldom": "^0.6.0"
    }
}
